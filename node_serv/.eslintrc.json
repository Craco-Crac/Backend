{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-plugin/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "ignoreRestSiblings": true
            }
        ]
    },
    "env": {
        "node": true,
        "es6": true
    }
}